    {
      "cell_type": "code",
      "source": [
        "df = df.drop('bmi', axis = 1)"
      ],
      "metadata": {
        "id": "Ygpxj_Nls6qF"
      },
      "execution_count": 18,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def calculate_bmi(row):\n",
        "    weight_kg = row['weight']\n",
        "    height_cm = row['height']  # Изменяем высоту в сантиметры\n",
        "    height_m = height_cm / 100  # Переводим в метры\n",
        "    bmi = weight_kg / (height_m ** 2)\n",
        "    return bmi\n",
        "\n",
        "# Применяем функцию к каждой строке DataFrame\n",
        "df['bmi'] = df.apply(lambda x: calculate_bmi(x), axis=1)"
      ],
      "metadata": {
        "id": "pSUwIi2vtCCw"
      },
